{
  "version": 2,
  "name": "mag-drawing",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/plan/([^/]+)",
      "dest": "/plan/[token]"
    }
  ],
  "functions": {
    "app/**/*.tsx": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": "@supabase_anon_key"
  }
}
